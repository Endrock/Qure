					button_label: "ALERT ME WHEN IT'S BACK!",
					subscription_success_label: "You're in! We'll let you know when it's back.",
					footer_content: '',
					additional_styles: "@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap');",
