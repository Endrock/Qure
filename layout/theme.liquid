			{% section 'header' %}
		{% endunless %}

		{% if template.suffix contains "about-us" %}
			{{ content_for_layout }}
		{% else %}
			<main role="main" id="MainContent" class="page__wrapper {% if template != 'index' %}not-index{% endif %}  {% if template contains 'reasons' or product.handle contains "micro-infusion" %} reasons {% endif %}">
				{{ content_for_layout }}
			</main>
		{% endif %}

		{% unless page.handle == 'qure-micro-infusion-landing-page' or 'microinfusion-safety-guide' %}
			{% section 'newsletter' %}
		{% endunless %}

		{% unless page.handle == 'qure-micro-infusion-landing-page' or page.handle == 'microinfusion-safety-guide' or page.handle == 'qure-microinfusion-179' %}
