    <script>
        // Get all elements with class 'tq_form'
        const tqForms = document.querySelectorAll('.tq_form');
        tqForms.forEach(form => {
            form.addEventListener('click', () => {
                tqForms.forEach(form => {
                    form.classList.remove('active');
                });
                form.classList.add('active');
            });
        });
