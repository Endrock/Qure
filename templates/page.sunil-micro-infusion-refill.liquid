		<script>
			document.addEventListener('DOMContentLoaded', () => {
					// Event listener for all options
					document.querySelectorAll('input[type="radio"][name="serum"]').forEach(option => {
							option.addEventListener('click', (event) => {
									const optionId = event.target.value;
									updateButtonsHref(optionId, '.buy-button');
							});
					});

					// Event listener for all options
					document.querySelectorAll('input[type="radio"][name="serum-bottom"]').forEach(option => {
							option.addEventListener('click', (event) => {
									const optionId = event.target.value;
									updateButtonsHref(optionId, '.buy-button-bottom');
							});
					});
			});

			function updateButtonsHref(id, classname) {
					document.querySelectorAll(classname).forEach(button => {
							const href = new URL(button.href);
							href.searchParams.set('id', id);
							button.href = href.toString();
					});
			}
		</script>

