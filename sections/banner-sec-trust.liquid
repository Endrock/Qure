<section class="banner-sec trust">
{% if section.settings.heading !=blank %}
<span class="stock-ava">{{ section.settings.heading }}</span>
{% endif %}  
<div class="container">
  <div class="row">
    <div class="col-md-6 cus-col-2">
      <div class="banner-sec-cont">
        {% if section.settings.head !=blank %}
        <h2>{{ section.settings.head }}</h2>
        {% endif %}
        {{ section.settings.text }}
