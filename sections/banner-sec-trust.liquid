          <ul class="bullet-detail">
            {% for i in (1..6) %}
            {% capture text %}text_{{ i }}{% endcapture %}
            {% assign custom_text = section.settings[text] %}
             {% if custom_text !=blank %}
              <li>{{ custom_text }}</li>
             {% endif %} 
             {% endfor %} 
          </ul>
        </div>
