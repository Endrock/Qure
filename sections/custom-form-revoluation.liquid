<section class="revoluation">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="revoluation-con">
          <h2>{{section.settings.heading}}</h2>
          {{section.settings.content}}
        </div>
      </div>
    </div>
  </div>
</section>

{% schema %}
  {
    "name": "Revoluation Text",
    "settings": [
		{
			"type":"text",
			"id":"heading",
			"label":"Heading"
		},
		{
			"type":"richtext",
			"id":"content",
			"label":"Content"
		}
]
  }
{% endschema %}
