<section class="introduction">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="facial">{{ section.settings.content }}</div>
          <div class="intro-con">
            <h2>{{ section.settings.heading }}</h2>
          </div>
          <div class="review">
            <span class="reviews">
              {{ section.settings.svg }}
            </span>
            {{ section.settings.text }}
          </div>
          <ul class="intro-bullet">
            {% for i in (1..5) %}
              {% capture text %}content_{{ i }}{% endcapture %}
              {% assign content = section.settings[text] %}
              {% if content !=blank %}
                <li>{{ content }}</li>
             {% endif %} 
            {% endfor %}  
          </ul>
          {% if section.settings.button !=blank %}
          <a href="javascript:void(0)" class="system-btn" rel="noindex nofollow">{{ section.settings.button }}
            <span class="btns__arrow">
              <svg width="10" height="16" viewBox="0 0 10 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M1 1L8 8L1 15" stroke="white" stroke-width="2"></path>
              </svg>
            </span>
          </a>
          {% endif %}    
        </div>
      </div>
    </div>
  </section>
