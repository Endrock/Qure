
<div class="banne-fetaure">
<h4>{{ section.settings.heading }} </h4>
  <ul>
    {% for block in section.blocks %}
    {% if block.settings.image !=blank %}
    <li>
      <img src="{{ block.settings.image| img_url:'large' }}">
    </li>
      {% endif %}

    {% endfor %}
  </ul>
</div>
{% schema %}
  {
    "name": "Banne Feature",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading Label"        
      }
      
    ],
    "blocks":[
      {
        "type": "banner",
        "name": "Image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"        
          }
        ]
        
      }
    ]
  }
{% endschema %}