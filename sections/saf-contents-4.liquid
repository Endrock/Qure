<section class="contents-4">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h2 class="contents-title"><span class="step-num">{{ section.settings.pre-treatment-num }}</span>{{ section.settings.pre-treatment-title }}</h2>
        <ul>
           {% for block in section.blocks %}  
          <li>{{block.settings.content-text}}</li>
           {% endfor %}        
        </ul>
      </div>
    </div>
  </div>
</section>
{% schema %}
  {
    "name": "Pre treatment",
    "settings": [
      {
      "type": "text",
      "id": "pre-treatment-num",
      "label": "Pre treatment Number"
    },
    {
      "type": "text",
      "id": "pre-treatment-title",
      "label": "Pre treatment Title"
    }
    ],
      "blocks": [
      {
        "type": "text",
        "name": "Pre treatment Description",
        
        "settings": [
          {
            "type":"textarea",
            "id":"content-text",
            "label":"Content Text"
          }
        ]
      }
      ],
        "presets": [
      {
        "name": "Pre treatment",
        "category": "Custom"
      }
      ]
  }
{% endschema %}