<span class="treatment-stock">{{ section.settings.main_heading }}</span>
<div class="landing-treatment landing-treatment-main-view mobile">
  <div class="container">
    <div clas="row">
      <div class="col-md-12">
        <div class="treatment-title">
          <h2>{{ section.settings.heading }}</h2>
          {{ section.settings.text }}
          <p>{{ section.settings.content }}</p>
        </div>
        <div class="new-supply">
          <div class="row">
            {% for block in section.blocks %}
              {% if block.type == 'first_month' %}
                <div class="col-md-6">
									<a href="#" name="scroll-area-mobile"></a>
                  <div class="new-image" id="aaaaqqqq4">
                    {% if block.settings.image != blank %}
                     <img class="lcp" src="https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Group_2058.png?v=1690892781">
                    {% endif %}
                  </div>
                </div>
              {% endif %}
              {% if block.type == 'three_month' %}
                <div class="col-md-6">
                  <div class="supply-detail">
                    <div class="supply-month">
                      {% for i in (1..3) %}
                        <div
                          class="supply-link {% if forloop.index == 1 %}active{% endif %} choice"
                          data-varid="{% if forloop.index == 1 %}3{% elsif forloop.index == 2%}2{% elsif forloop.index == 3%}1{% endif %} Month Supply"
                        >
                          {% if forloop.index == 1 %}
                            {% if block.settings.center_heading != blank %}
                              <span class="choice">{{ block.settings.center_heading }}</span>
                            {% endif %}
                          {% endif %}

                          {% capture price %}price_{{ i }}{% endcapture %}
                          {% capture content %}content_{{ i }}{% endcapture %}
                          {% capture old_price %}old_price_{{ i }}{% endcapture %}
                          {% capture new_price %}new_price_{{ i }}{% endcapture %}
                          {% capture save_price %}save_price_{{ i }}{% endcapture %}
                          {% capture text %}text_{{ i }}{% endcapture %}
                          {% capture tab_content %}tab_text_{{ i }}{% endcapture %}

                          {% assign prices = block.settings[price] %}
                          {% assign cus_content = block.settings[content] %}
                          {% assign old__price = block.settings[old_price] %}
                          {% assign new__price = block.settings[new_price] %}
                          {% assign save__price = block.settings[save_price] %}
                          {% assign sub_content = block.settings[text] %}
                          {% assign tab__content = block.settings[tab_content] %}

                          <div class="title-price">
                            <div class="new-title">
                              {% if prices != blank %}
                                <h5>{{ prices }}</h5>
                              {% endif %}
                              {% if cus_content != blank %}
                                <p>{{ cus_content }}</p>
                              {% endif %}
                            </div>
                            <div class="new-price">
                              <p>
                                {% if old__price != blank %}
                                  <sapn class="old-price">{{ old__price }}</sapn>
                                {% endif %}
                                {% if new__price != blank %}
                                  <span class="new-price">{{ new__price }}</span>
                                {% endif %}
                                {% if save__price != blank %}
                                  <span class="newsave-price">{{ save__price }}</span>
                                {% endif %}
                              </p>
                            </div>
                          </div>
                          <div class="supp-acc">
                            <div class="set" id="accordion-no-dek-1">
                              {% if sub_content != blank %}
                                <a href="javascript:void(0)" rel="noindex nofollow">
                                  {{ sub_content }}
                                  <i class="fa fa-chevron-down"></i>
                                </a>
                              {% endif %}
                              {% if tab__content != blank %}
                                <div class="content">
                                  {{ tab__content }}
                                </div>
                              {% endif %}
                            </div>
                          </div>
                        </div>
                      {% endfor %}
                    </div>
                    <div class="new-treatment" id="aaaaqqqq3">
                      {% if block.settings.chooes_heading != blank %}
                        <h3>{{ block.settings.chooes_heading }}</h3>
                      {% endif %}
                      <ul>
                        {% for i in (1..3) %}
                          {% capture custom_text_add %}cus_text_{{ i }}{% endcapture %}
                          {% capture custom_text_heading %}content_custom_{{ i }}{% endcapture %}

                          {% assign text_head = block.settings[custom_text_add] %}
                          {% assign text__custom_heda = block.settings[custom_text_heading] %}

                          <li class="round{{ forloop.index }}" {% if forloop.index == 1 %}style="border-left:1px solid #CCE5DD"{% endif %}>
                            <a
                              href="javascript:void(0)"
                              class="choose-link {% if forloop.index == 1 %}active{% endif %}"
															rel="noindex nofollow"
                            >
                              <div class="rounder"><span></span></div>
                              {% if text_head != blank %}
                                <h5>{{ text_head }}</h5>
                              {% endif %}
                              {{ text__custom_heda }}
                            </a>
                          </li>
                        {% endfor %}
                      </ul>
                    </div>
                    <div class="new-pay">
                      {% if block.settings.options_heading_1 != blank %}
                        <h3>{{ block.settings.options_heading_1 }}</h3>
                      {% endif %}
                      <ul>
                        <li>
                          <a href="javascript:void(0)" class="pay-link active" rel="noindex nofollow">
