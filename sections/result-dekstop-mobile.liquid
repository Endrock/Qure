<section class="result-sli">
    <h2>{{ section.settings.heading }}</h2>
    <div class="swiper-container mySwiper">
      <div class="swiper-wrapper slide-new">
        {% for block in section.blocks %}
        <div class="swiper-slide revi-detail">
          <div class="rev-img">
            <img src="{{ block.settings.image | img_url:'large' }}" alt="">
            {% if block.settings.before != blank %}
          <span class="before">{{ block.settings.before  }}</span>
                 {% endif %}
            {% if block.settings.after != blank %}
          <span class="after">{{ block.settings.after  }}</span>
              {% endif %}
          </div>
          <div class="rev-detial">
            <div class="rev-info">
              <div class="rev-infos">
                <div class="rev-names">
                 <img src="{{ block.settings.rev_img | img_url:'large' }}" alt="">
                  <span>{{ block.settings.rev_name  }}</span>
                </div>
                <div class="rev-veri">
                  <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
