          </svg>
            </div>
           <div class="cus-class"> {{ block.settings.content }}</div>
          </div>
        </div>
        {% endfor %}  
      </div>
       <div class="swiper-button-next"></div>
      <div class="swiper-button-prev"></div>
    </div>
  </section>
{% schema %}
  {
  "name": "Result Desktop Mobile",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading Label"
    }
  ],
  
  "blocks": [
    {
      "type": "image_width_text",
      "name": "Image width Text",
      "settings":[
        {
          "type": "image_picker",
          "id": "image",
          "label": "image"
        },
        {
          "type": "text",
          "id": "before",
          "label": "Before Heading"
          
        },
        {
          "type": "text",
          "id": "after",
          "label": "After Heading"
        },
        {
          "type": "image_picker",
          "id": "rev_img",
          "label": "image"
        },
        {
          "type": "text",
          "id": "rev_name",
          "label": "After Heading"
        },
        {
          "type": "richtext",
          "id": "content",
          "label": "Text"
        },
        {
          "type": "image_picker",
          "id": "rev_image",
          "label": "Rev Image"
        },
        {
          "type": "text",
          "id": "rev_text",
          "label": "Rev Text"
        },
        {
          "type": "image_picker",
          "id": "ver_image",
          "label": "Ver Image"
        },
        {
          "type": "text",
          "id": "ver_text",
          "label": "Ver Text"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Result Desktop Mobile"
    }
  ]
}
{% endschema %}