<div class="land-container">
  <div class="land-text__title-wrapper upsell-second__title-wrapper">
    <h2 class="landing-title upsell-second__title">{{section.settings.title}}</h2>
    {{section.settings.subtitle}}
    <div class="bf-compare__button-wrapper">
      <div class="bf-compare__button color-btn" data-for="mask">Q-Rejuvalight Pro LED Mask</div>
      <div class="bf-compare__button transparent-btn" data-for="micro">Micro-Infusion System</div>
    </div>
  </div>
  <div class="upsell-second__items-wrapper bf-upsell-mask active">
    <div class="upsell-second__item" data-num="1">
      <div class="upsell-second__top">
        <div class="upsell-second__img-wrapper">
          <img class="upsell-second__img" src="{{section.settings.image1 |img_url: 'master'}}" alt="">
          <div class="upsell-second__img--text"><span class="bf-desktop"><b>Qure</b> LED Light Therapy Mask</span><span class="bf-mobile">Qure</span></div>
          <div class="upsell-second__vs-img">VS</div>
        </div>
      </div>
      <div class="upsell-second__bottom">
        <div class="upsell-second__price">{{section.settings.price}}</div>
        <div class="upsell-second__label">{{section.settings.label}}</div>
        <div class="upsell-second__text">{{section.settings.text}}</div>
        {% comment %}
        <div class="upsell-second__line-wrapper"><span class="upsell-second__line"></span></div>
        {% endcomment %}
        <div class="upsell-second__text--list qure-list">
          {%capture tickGreen%}
          <svg width="15" height="11" viewBox="0 0 15 11" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M1 4.63636L5.36364 9L13.3636 1" stroke="#008763" stroke-width="2"/>
          </svg>
          {%endcapture%}
          {%capture crossRed%}
          <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M5 14L14 5" stroke="#C92127" stroke-width="2"/>
            <path d="M5 5L14 14" stroke="#C92127" stroke-width="2"/>
          </svg>
          {%endcapture%}
          {%for block in section.blocks%}
          {%if block.settings.text_list != blank%}
          {% if block.type == "paragraph" %}
          <div class="list__text-wrapper bf-desktop">
            {{tickGreen}}
            {{block.settings.text_list}}
          </div>
          <div class="list__text-wrapper bf-mobile">
            {{tickGreen}}
            {{block.settings.text_list_mob}}
          </div>
          {% endif %}
          {%endif%}
          {%endfor%}
        </div>
      </div>
    </div>
    <div class="upsell-second__item" data-num="2">
      <div class="upsell-second__top">
        <div class="upsell-second__img-wrapper">
          <img class="upsell-second__img" src="{{section.settings.image2 |img_url: 'master'}}" alt="">
          <div class="upsell-second__img-competitor--text">Dermatologist / Clinic</div>
        </div>
      </div>
      <div class="upsell-second__bottom">
        <div class="upsell-second__price">{{section.settings.price_comp}}</div>
        <div class="upsell-second__label">
          <span class="bf-desktop desktop-wrapper">{{section.settings.label_comp}}</span>
          <span class="bf-mobile mobile-wrapper">{{section.settings.label_comp_mob}}</span>
        </div>
        <div class="upsell-second__text">{{section.settings.text_comp}}</div>
        {% comment %}
        <div class="upsell-second__line-wrapper"><span class="upsell-second__line"></span></div>
        {% endcomment %}
        <div class="upsell-second__text--list competitor-list">
          {%for block in section.blocks%}
          {%if block.settings.text_list_comp != blank%}
          {% if block.type == "paragraph" %}
          <div class="list__text-wrapper bf-desktop">
            {{crossRed}}
            {{block.settings.text_list_comp}}
          </div>
          <div class="list__text-wrapper bf-mobile">
            {{crossRed}}
            {{block.settings.text_list_comp_mob}}
          </div>
          {% endif %}
          {%endif%}
          {%endfor%}
        </div>
      </div>
    </div>
  </div>
  <div class="upsell-second__items-wrapper bf-upsell-microinfusion">
    <div class="upsell-second__item" data-num="1">
      <div class="upsell-second__top">
        <div class="upsell-second__img-wrapper">
          <img class="upsell-second__img" src="{{section.settings.image1_2 |img_url: 'master'}}" alt="">
          <div class="upsell-second__img--text"><span class="bf-desktop">Qureâ€™s At-Home <br>Micro-Infusion System</span><span class="bf-mobile">Qure</span></div>
          <div class="upsell-second__vs-img">VS</div>
        </div>
      </div>
      <div class="upsell-second__bottom">
        {% comment %}
        <div class="upsell-second__line-wrapper"><span class="upsell-second__line"></span></div>
        {% endcomment %}
        <div class="upsell-second__text--list qure-list">
          {%capture tickGreen%}
          <svg width="15" height="11" viewBox="0 0 15 11" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M1 4.63636L5.36364 9L13.3636 1" stroke="#008763" stroke-width="2"/>
          </svg>
          {%endcapture%}
          {%capture crossRed%}
          <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M5 14L14 5" stroke="#C92127" stroke-width="2"/>
            <path d="M5 5L14 14" stroke="#C92127" stroke-width="2"/>
          </svg>
          {%endcapture%}
          {%for block in section.blocks%}
          {% if block.type == "paragraph_micro" %}
          <div class="list__text-wrapper bf-desktop">
            {{tickGreen}}
            {{block.settings.text_list_2}}
          </div>
          <div class="list__text-wrapper bf-mobile">
            {{tickGreen}}
            {{block.settings.text_list_mob_2}}
          </div>
          {% endif %}
          {%endfor%}
        </div>
      </div>
    </div>
    <div class="upsell-second__item" data-num="2">
      <div class="upsell-second__top">
        <div class="upsell-second__img-wrapper">
          <img class="upsell-second__img" src="{{section.settings.image2_2 |img_url: 'master'}}" alt="">
          <div class="upsell-second__img-competitor--text">Dermatologist / Clinic</div>
        </div>
      </div>
      <div class="upsell-second__bottom">
        {% comment %}
        <div class="upsell-second__line-wrapper"><span class="upsell-second__line"></span></div>
        {% endcomment %}
        <div class="upsell-second__text--list competitor-list">
          {%for block in section.blocks%}
          {% if block.type == "paragraph_micro" %}
          <div class="list__text-wrapper bf-desktop">
            {{crossRed}}
            {{block.settings.text_list_comp_2}}
          </div>
          <div class="list__text-wrapper bf-mobile">
            {{crossRed}}
            {{block.settings.text_list_comp_mob_2}}
          </div>
          {% endif %}
          {%endfor%}
        </div>
      </div>
    </div>
  </div>
</div>




