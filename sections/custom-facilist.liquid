<div class="container-fluid Facialist-banner">
  <div class="container">
  <div class="row">
    <div class="col-lg-7 col-md-4"></div>
    <div class="col-lg-5 col-md-8">
      <div class="facialist-contant">
        <h2>{{section.settings.title}}</h2>
        <div class="facialist-box02">
          <div class="icon-box02">{{ section.settings.image_1 | img_url: 'master' | img_tag }}</div>
          <div class="icon-box03">{{section.settings.title1}}</div>
        </div> 
        
          <div class="facialist-box02">
          <div class="icon-box02">{{ section.settings.image_2 | img_url: 'master' | img_tag }}</div>
          <div class="icon-box03">{{section.settings.title2}}</div>
