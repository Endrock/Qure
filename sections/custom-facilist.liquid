          <div class="icon-box03">{{section.settings.title3}}</div>
        </div> 
        
          <div class="facialist-box02">
          <div class="icon-box02">{{ section.settings.image_4 | img_url: 'master' | img_tag }}</div>
          <div class="icon-box03">{{section.settings.title4}}</div>
        </div> 
                  
      </div>
    </div>    
  </div>
</div>
</div>
