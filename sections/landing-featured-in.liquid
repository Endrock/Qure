<div class="land-container">
  <div class="featured-in__title-wrapper">
    <h3 class="featured-in__title">{{section.settings.title}}</h3>
  </div>
  <div class="featured-in__inner">
    {%for block in section.blocks%}
    <div class="featured-in__images">
      <img src="{{ block.settings.image | img_url: 'master'}}" class="featured-in__img">
    </div>
    {%endfor%}
  </div>
</div>
{% schema %}
  {
    "name": "Featured in",
	"class": "landing-section featured-in-section",
    "settings": [
		{
			"type": "text",
			"id": "title",
			"label": "Title",
			"default": "FEATURED IN"
		}
	],
	"blocks": [
		{
			"type": "image",
			"name": "Image",
			"settings": [
				{
					"type": "image_picker",
					"id": "image",
					"label": "Image"
				}
			]
		}
	]
  }
{% endschema %}
