<div class="bakuchiol-section">
  <div class="swiper-container bakuch-slider">
    <div class="swiper-wrapper">


      {% for block in section.blocks %}
      <div class="swiper-slide">
        <div class="bakuchoil-info">
          <div class="bak-image">
            <img src="{{block.settings.prd_img | img_url:'master' }}">
          </div>
          <div class="bak-info">
            <h2>{{block.settings.prd_tlt }}</h2>
            {{ block.settings.prd_content}}

            <div id="test-popup-slider-{{ forloop.index }}" class="white-popup mfp-hide">
              <h2>{{ block.settings.pop_hng }}</h2>
              {{ block.settings.popup_content }}
            </div>
            <a href="#test-popup-slider-{{ forloop.index }}" class="open-popup-link bak-btn">{{ block.settings.pop_text }}<img src="https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Group_502.png?v=1622025966"></a>
          </div>
        </div>
      </div>
      {% endfor %}       
    </div>
    <div class="swiper-pagination"></div>
  </div>
</div>    
<div class="cus-nav">
  <div class="swiper-pagination"></div>
</div>
