{% schema %}
{
"name": "Ingredient Slider",
"settings": [

{
"type":"header",
"content":"ingredient"
},
{
"type":"textarea",
"id":"ingredient",
"label":"Ingredient"
}
],

"blocks": [
{
"type": "text",
"name": "Slider",
"settings": [

{
"id": "prd_img",
"type": "image_picker",
"label": "Choose Logo"
},
{
"id": "prd_tlt",
"type": "text",
"label": "Heading"
},
{
"id": "prd_content",
"type": "richtext",
"label": "Content"
},
{
"type":"header",
"content":"Popup"
},
{
"type":"text",
"id":"pop_text",
"label":"Popup Text"
},
{
"type":"text",
"id":"pop_hng",
"label":"Popup Heading"
},
{
"type":"textarea",
"id":"popup_content",
"label":"Popup Content"
}
]
}
]
}
{% endschema %}