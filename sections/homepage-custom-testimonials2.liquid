<div class="section-customer-review generic-slider-block">
  <div class="container">
    <!--     <h2 class="g-canela-heading">{{section.settings.title}}</h2> -->
    
        {% unless template contains 'product' %}
    <span>Real and unfiltered qure reviews and results</span>
    {% endunless %}
    

    <h2 class="g-canela-heading">Does it work? Is it legit? <br>We'll let our customers take it from here.</h2>

    <div class="customer-review-slider">
      {% for block in section.blocks %}
      <div class="blog-slides-item">
        {% include 'responsive-image' with
                  image: block.settings.testimonials_image,
        max_width: 1920,
        max_height: 3000
        %}
        <div class="blog-slides-text">
          {{block.settings.testimonial_text}}
          <div class="a-customer-name">{{block.settings.testimonial_author}}</div>
        </div>
      </div>
      {% endfor %}
    </div>
        
  </div>
</div>
