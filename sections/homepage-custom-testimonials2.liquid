    font-size: 55px;
    color: #333333;
    font-family: "Canela";
  }
  .our-purpose .purpose-banner .purpose-contant p{
    font-size: 18px;
    line-height: 30px;
    color: #333333;
    font-weight: 300;
    text-align: center;
    margin-bottom: 25px;
  }
