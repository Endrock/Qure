{% schema %}
{
"name": "Custom testimonials",
"class": "home-testimonials",
"settings": [
{
"type": "text",
"id": "title",
"label": "Section title",
"default": "Qure changes everything"
}
],

"blocks": [
{
"type": "testimonials",
"name": "testimonial",
"settings": [
{
"type": "image_picker",
"id": "testimonials_image",
"label": "Image"
},
{
"type": "richtext",
"id": "testimonial_text",
"label": "Testimonial"
},
{
"type": "text",
"id": "testimonial_author",
"label": "Author"
}
]
}
],

"presets": [{
"name": "Custom testimonials",
"category": "Promotional"
}]
}
{% endschema %}


