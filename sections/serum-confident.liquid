<style> 
  .mask-confident {
    padding: 98px 0 130px;
    background-color: #fff;
}
  .mask-confident .confident-main {
    justify-content: center;
}
  .mask-confident h2 {
    font-style: normal;
    font-weight: 600;
    font-size: 38px;
    line-height: 55px;
    text-align: center;
    letter-spacing: 1px;
    color: #000000;
    font-family: 'Canela-Thin';
    width: 100%;
    margin: 0 auto 30px;
}
  .confident-main {
    display: flex;
    align-items: center;
}
.confident-wrap {
    margin: 0;
    position: relative;
    padding-bottom: 179%;
    width: 100%;
    overflow: hidden;
}
.confident-main .confident-slid {
    width: 180px;
    margin-right: 10px;
    height: 100%;
    position: relative;
}
.confident-wrap iframe {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: transparent;
    min-height: auto;
    border-radius: 8px;
}
.confident-wrap .js-videoPoster {
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background-size: 100%;
    border: none text-indent: -999em;
    overflow: hidden;
    opacity: 1;
    -webkit-transition: opacity 800ms, height 0s;
    -moz-transition: opacity 800ms, height 0s;
    transition: opacity 800ms, height 0s;
    -webkit-transition-delay: 0s, 0s;
    -moz-transition-delay: 0s, 0s;
    transition-delay: 0s, 0s;
    border: transparent;
    background-color: transparent;
    right: 0;
    bottom: 0;
    padding: 0;
}
.confident-wrap .js-videoPoster:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 35px;
    height: 35px;
    margin: -15px 0 0 -20px;
    border: 3px solid #fff;
    border-radius: 100%;
    -webkit-transition: border-color 300ms;
    -moz-transition: border-color 300ms;
    transition: border-color 300ms;
}
.confident-wrap .js-videoPoster:after {
    content: '';
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    margin: -5px 0 0 -6px;
    border-left: 10px solid #fff;
    border-top: 7px solid transparent;
    border-bottom: 7px solid transparent;
    -webkit-transition: border-color 300ms;
    -moz-transition: border-color 300ms;
    transition: border-color 300ms;
}
.videoWrapperActive .videoPoster {
    opacity: 0;
    height: 0;
    -webkit-transition-delay: 0s, 800ms;
    -moz-transition-delay: 0s, 800ms;
    transition-delay: 0s, 800ms;
}
  .confident-wrap .js-videoPoster img {
    width: 100%;
    height: 100%;
}
.confident-main .confident-slid{
  margin-right: 15px;
}
.confident-main .confident-slid:last-child{
  margin-right: 0px;
}
.confident-wrap video {
    object-fit: cover;
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: transparent;
    min-height: auto;
    border-radius: 8px;
}
[data-testid="card-component"] {
    display: none;
}

@media (min-width: 992px){
   .dual-action {
      padding: 130px 0 85px;
  } 
}
@media (max-width:991px){
    .confident-main .confident-slid {
      margin-right: 15px;
    }
  .mask-confident .slick-next{
    top: auto;
    bottom: -50px;
    width: auto;
    right: 44%;
  }
  .mask-confident .slick-next:before, .mask-confident .slick-prev:before{
    content: none;
  }
  .mask-confident .slick-next svg,
  .mask-confident .slick-prev svg{
    width: 35px;
    height: 25px;
  }
   .mask-confident .slick-prev{
    top: auto;
    bottom: -46px;
    transform: rotate(-180deg) !important;
    width: 35px;
    left: 42%;
  }
  .mask-confident h2 {
    font-size: 28px;
    line-height: 38px;
    width: 100%;
    padding: 0 10px;
    margin: 0px auto 29px;
  }
  .mask-confident .confident-main{
    width:100%;

  }
  .mask-confident .confident-main .confident-slid:last-child {
    margin-right: 15px;
	}

	.mask-confident {
		padding: 50px 0 60px;
	}

}
@media (max-width:767px){
  .mask-confident .slick-next {
    right: 21px;
    bottom: -10px;
    width: 8px;
    z-index:1;
}
  .mask-confident .slick-prev {
    left: 10px;
    bottom: 0;
    width: 8px;
    z-index:1;
}
.mask-confident ul.slick-dots li {
    background: rgba(0, 0, 0, 0.1);
    width: 10px;
    height: 10px;
    border-radius: 100%;
}
.mask-confident {
    padding: 36px 0 0px !important;
}
.confident-main .confident-slid{
  width: 50%;
}
.mask-confident .confident-slid{
  display:none;
}
.mask-confident .confident-slid:nth-child(1){
  display:block;
} 
.mask-confident .confident-slid:nth-child(2){
  display:block;
}
.mask-confident .confident-slid.slick-slide{
 display:block;
}
}

