<style>
  {{ settings.font_heading | font_face }}
  {{ settings.font_body | font_face }}

  {%- assign font_heading_thin = settings.font_heading | font_modify: 'weight', '100' -%}
  {%- assign font_heading_light = settings.font_heading | font_modify: 'weight', '300' -%}
  {%- assign font_heading_medium = settings.font_heading | font_modify: 'weight', '500' -%}
  {%- assign font_heading_bold = settings.font_heading | font_modify: 'weight', '700' -%}

  {%- assign font_body_light = settings.font_body | font_modify: 'weight', '300' -%}
  {%- assign font_body_medium = settings.font_body | font_modify: 'weight', '600' -%}
  {%- assign font_body_bold = settings.font_body | font_modify: 'weight', '700' -%}


  {{ font_heading_thin | font_face: font_display: 'optional' }}
  {{ font_heading_light | font_face: font_display: 'optional' }}
  {{ font_heading_medium | font_face: font_display: 'optional' }}
  {{ font_heading_bold | font_face: font_display: 'optional' }}

  {{ font_body_light | font_face: font_display: 'optional' }}
  {{ font_body_medium | font_face: font_display: 'optional' }}
  {{ font_body_bold | font_face: font_display: 'optional' }}

	@font-face {
		font-family: icomoon;
		src: url({{ 'icomoon.woff' | asset_url }}) format("woff");
		font-weight: 400;
		font-style: normal;
		font-display: auto;
	}

	@font-face {
		font-family: 'Calluna-Regular';
		src: url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Calluna-Regular.eot?v=1600767492');
		src: url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Calluna-Regular.eot?v=1600767492?#iefix') format('embedded-opentype'),
			url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Calluna-Regular.woff2?v=1600767513') format('woff2'),
			url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Calluna-Regular.woff?v=1600767529') format('woff'),
			url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/Calluna-Regular.ttf?v=1600767545') format('truetype'),
			url('Calluna-Regular.svg#Calluna-Regular') format('svg');
		font-weight: normal;
		font-style: normal;
		font-display: swap;
	}
	@font-face {font-family: "Canela"; 
		src: url("//db.onlinewebfonts.com/t/b1edfa746f416280b3502ba39df2c3ae.eot"); 
		src: url("//db.onlinewebfonts.com/t/b1edfa746f416280b3502ba39df2c3ae.eot?#iefix") format("embedded-opentype"), 
			url("//db.onlinewebfonts.com/t/b1edfa746f416280b3502ba39df2c3ae.woff2") format("woff2"),
			url("//db.onlinewebfonts.com/t/b1edfa746f416280b3502ba39df2c3ae.woff") format("woff"), 
			url("//db.onlinewebfonts.com/t/b1edfa746f416280b3502ba39df2c3ae.ttf") format("truetype"), 
			url("//db.onlinewebfonts.com/t/b1edfa746f416280b3502ba39df2c3ae.svg#Canela") format("svg"); 
	}
	@font-face {
			font-family: 'canela_webmedium';
			src: url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/new_canela-medium-reduced.woff?v=1684746857') format('woff2'),
					url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/new_canela-medium-reduced.woff2?v=1684746857') format('woff');
			font-weight: normal;
			font-style: normal;

	}
	@font-face {
			font-family: 'canela_weblight';
			src: url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/new_canela-light-reduced.woff2?v=1684746857') format('woff2'),
					url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/new_canela-light-reduced.woff?v=1684746857') format('woff');
			font-weight: normal;
			font-style: normal;

	}
	@font-face {
			font-family: 'canelaregular';
			src: url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/new_canela-regular.woff2?v=1684746857') format('woff2'),
					url('https://cdn.shopify.com/s/files/1/0441/1431/3365/files/new_canela-regular.woff?v=1684746857') format('woff');
			font-weight: normal;
			font-style: normal;
	}

	@font-face{
		font-family:'slick';
		font-weight:400;
		font-style:normal;
		font-display: swap;
		src:url("{{ 'slick.woff' | asset_url }}") format('woff');
	}

	@font-face{
		font-family:'swiper-icons';
		src:url('data:application/font-woff;charset=utf-8;base64, ') format('woff');
		font-weight:400;
		font-style:normal;
		font-display: swap;
	}

	@font-face{
		font-family:'FontAwesome';
		src:url("{{ 'fontawesome-webfont.woff2' | asset_url }}") format('woff2');
		font-weight:normal;
		font-style:normal;
		font-display: swap;
	}

	@font-face {
		font-family: 'Canela-bold';
		src: url("{{ 'Canela-Bold.woff' | asset_url }}") format("woff");
		font-display: swap;
	}

	@font-face {
		font-family: 'Poppins-SemiBold';
		src: url("{{ 'Poppins-SemiBold.woff' | asset_url }}") format("woff");
		font-display: swap;
	}
	
	@font-face {
		font-family: 'Canela-Medium-Reduced';
		src: url('//cdn.shopify.com/s/files/1/0441/1431/3365/t/62/assets/Canela-Medium-Reduced.woff2?v=35850876921324681131649412061') format("woff2");
		font-display: swap;
	}





