{% comment %} This is examples "How to load various objects in the preload mode" {% endcomment %}
{% comment %}
<link rel="preload" as="image" href="hero-image.jpg" fetchpriority="high">
<link rel="preload" href="{{ '' | asset_url }}" as="style" fetchpriority="low">
<link rel="preload" href="{{ '' | asset_url }}" as="font" type="font/woff2" fetchpriority="low" crossorigin>
<link rel="preload" href="{{ '' | asset_url }}" as="script" fetchpriority="low">
<link rel="preload" href="https://fonts.googleapis.com/css?family=Roboto&display=swap" as="style" fetchpriority="low">
<link rel="preload" href="https://use.typekit.net/urw2bvy.css" as="style" onload="this.onload=null;this.rel='stylesheet'" fetchpriority="low">
{% endcomment %}

<!-- preload_image_lcp -->

<link rel="preload" href="{{ '__app-styles.css' | asset_url }}" as="style" fetchpriority="low">

<link rel="preload" href="https://qureskincaredns.com/_sunil_homepage_update/assets/_sunil_swiper-bundle-min.css" as="style" fetchpriority="low">
<link rel="preload" href="https://qureskincaredns.com/_sunil_homepage_update/assets/_sunil_new-homepage.css" as="style" fetchpriority="low">
<link rel="preload" href="https://qureskincaredns.com/_sunil_homepage_update/assets/_sunil_cartChanges.css" as="style" fetchpriority="low">

{%- if request.host contains '/products/q-rejuvalight-pro-facewear' -%}
	<link rel="preload" href="{{ 'landing-page.css' | asset_url }}" as="style" fetchpriority="low">
{%- endif -%}

{%- if request.host contains '/pages/micro-infusion-refill' -%}
	<link rel="preload" href="{{ 'refill.css' | asset_url }}" as="style" fetchpriority="low">
{%- endif -%}